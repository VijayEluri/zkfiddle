<ehcache>
	<diskStore path="C:/temp"/>
	<defaultCache
	    maxElementsInMemory="10000"
	    eternal="true"
	    timeToIdleSeconds="120"
	    timeToLiveSeconds="120"
	    overflowToDisk="true"
	    />
	<cache
	    name="org.hibernate.cache.UpdateTimestampsCache"
	    maxElementsInMemory="5000"
	    eternal="true"
	    overflowToDisk="true"/>
		
</ehcache>